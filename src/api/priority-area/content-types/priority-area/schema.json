{
  "kind": "collectionType",
  "collectionName": "priority_areas",
  "info": {
    "singularName": "priority-area",
    "pluralName": "priority-areas",
    "displayName": "PriorityArea",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "IgadActions": {
      "type": "component",
      "repeatable": true,
      "component": "priority-areas.action-items"
    },
    "memberStateActions": {
      "type": "component",
      "repeatable": true,
      "component": "priority-areas.action-items"
    },
    "expectedOutcomes": {
      "type": "component",
      "repeatable": true,
      "component": "priority-areas.action-item-list"
    },
    "stackholders": {
      "type": "component",
      "repeatable": true,
      "component": "priority-areas.links"
    },
    "partners": {
      "type": "component",
      "repeatable": true,
      "component": "priority-areas.links"
    },
    "header": {
      "type": "text"
    },
    "mainContent": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
